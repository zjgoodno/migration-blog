<% page.posts.each(function(post) { %>
<div class="container">
  <div class="post">
    <header class="post-title">
      <h2><%- post.title %></h2>
      <p><%- moment(post.date).format("dddd, MMM D") %> - <%-post.location%></p>
    </header>
    <div class="post-content">
      <%- post.content %>
    </div>
  </div>
</div>
<%})%>
